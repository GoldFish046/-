<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernateEntity.Borrow" table="borrow" schema="booksmanagersystem">
        <property name="bookindex" column="bookindex"/>
        <property name="readerindex" column="readerindex"/>
        <property name="begin" column="begin"/>
        <property name="end" column="end"/>
    </class>
</hibernate-mapping>